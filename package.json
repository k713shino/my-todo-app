{
  "name": "my-todo-app",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:prod": "next build",
    "build:vercel": "npx prisma generate && next build",
    "vercel-build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:fix": "next lint --fix --quiet",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "redis:cli": "redis-cli -h localhost -p 6379",
    "redis:monitor": "redis-cli -h localhost -p 6379 monitor",
    "redis:info": "redis-cli -h localhost -p 6379 info",
    "cache:clear": "node --experimental-modules scripts/cache-stats.js clear",
    "cache:stats": "node --experimental-modules scripts/cache-stats.js",
    "postinstall": "npx prisma generate || echo 'Prisma generation failed, continuing...'"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ioredis": "^5.3.2",
    "next": "15.3.5",
    "next-auth": "^4.24.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.4.1",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.19.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "prisma": "^6.12.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5"
  }
}
