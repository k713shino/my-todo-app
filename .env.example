# AWS RDS PostgreSQL接続URL (Vercel/Lambda最適化版)
DATABASE_URL="postgresql://username:password@your-db-host.region.rds.amazonaws.com:5432/your_database?connection_limit=1&pool_timeout=20&connect_timeout=30&socket_timeout=30&sslmode=require"

# Prisma Shadow Database (マイグレーション用)
SHADOW_DATABASE_URL="postgresql://username:password@your-db-host.region.rds.amazonaws.com:5432/your_database_shadow"

# 🔴 Redis接続URL（新規追加）
REDIS_URL="redis://default:your-redis-password@your-redis-host.upstash.io:6379"
REDIS_PASSWORD="your-redis-password"
UPSTASH_REDIS_REST_URL="https://your-redis-host.upstash.io"
UPSTASH_REDIS_REST_TOKEN="your-upstash-redis-rest-token"

# NextAuth設定
NEXTAUTH_SECRET="your-nextauth-secret-key-32-characters-long"
NEXTAUTH_URL="https://your-app-domain.vercel.app"

# GitHub OAuth設定
GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"

# Google OAuth設定
GOOGLE_CLIENT_ID="your-google-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# クライアントサイドで使用するGoogle認証設定
NEXT_PUBLIC_GOOGLE_CLIENT_ID="your-google-client-id.apps.googleusercontent.com"

# 🔴 キャッシュ設定（新規追加）
CACHE_TTL=3600
ENABLE_CACHE=true

# 🔴 リアルタイム機能（新規追加）
ENABLE_REALTIME=true

# Build Environment Variables (for Vercel)
SKIP_ENV_VALIDATION="1"
NEXT_TELEMETRY_DISABLED="1"

NEXTAUTH_DEBUG=true


LAMBDA_API_URL="https://your-lambda-api-id.execute-api.region.amazonaws.com/prod"
NEXT_PUBLIC_LAMBDA_API_URL="https://your-lambda-api-id.execute-api.region.amazonaws.com/prod"

# 🔴 Lambda + Prisma最適化設定
DATABASE_MAX_CONNECTIONS=1
PRISMA_CONNECTION_TIMEOUT=5000
LAMBDA_WARMUP_ENABLED=true
LAMBDA_PERFORMANCE_MONITORING=true

# 🔴 Lambda DB経由でのデータベース接続を有効化
USE_LAMBDA_DB=true

# 🔵 インポート並列度（同時実行数・省略時は4）
IMPORT_CONCURRENCY=4

# Prismaログレベル制御（環境変数で管理）
DEBUG=prisma:error
PRISMA_LOG_LEVEL=error
